using System;
using System.Collections;

namespace StacksAndQueues
{
    class Program
    {
        static void Main(string[] args)
        {
            Stack S = new Stack();
            var input = Console.ReadLine();
            var charArray = input.ToCharArray();
            for (int i = 0; i < charArray.Length; i++)
            {
                S.Push(charArray[i]);
            }
            foreach (var item in S)
            {
                Console.Write(item);
            }
            
        }
    }
}
